{
  "name": "hackardennes",
  "private": true,
  "dependencies": {
    "bootstrap-sass": "~3.3.5",
    "modernizr": "~2.8.1",
    "font-awesome": "~4.4.0",
    "jquery-easing-original": "~1.3.2",
    "wowjs": "~1.1.2"
  },
  "overrides": {
    "bootstrap-sass": {
      "main": [
        "assets/stylesheets/_bootstrap.scss",
        "assets/fonts/bootstrap/*",
        "assets/javascripts/bootstrap.js"
      ]
    },
    "font-awesome": {
      "main": [
        "fonts/*",
        "scss/font-awesome.scss"
      ]
    },
    "jquery-easing-original": {
      "main": [
        "jquery.easing.js"
      ]
    },
    "wowjs": {
      "main": [
        "css/libs/animate.css",
        "dist/wow.js"
      ]
    }
  },
  "devDependencies": {
    "chai": "~3.4.2",
    "mocha": "~2.3.4"
  }
}
